@using FFCG.CrashCourse.Web.Models
@model IEnumerable<FFCG.CrashCourse.Web.Models.ObservationViewModel>

@{
    ViewBag.Title = "Observations";
}

<h2>Observations</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Value)
        </th>
        <th></th>
    </tr>

@foreach (ObservationViewModel item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Value)
        </td>
    </tr>
}

</table>
